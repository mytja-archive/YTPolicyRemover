{
  "name": "YTPolicyRemover",
  "description" : "ToS box remover from YT",
  "version": "1.0.1",
  "permissions": ["storage"],
  "author": "mytja",
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "run_at": "document_end",
    "js": ["remover.js"]
  }
  ],
  "browser_action": {
    "default_popup": "index.html",
  },
  "manifest_version": 2
}
